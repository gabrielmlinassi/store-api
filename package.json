    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "stripe": "^14.21.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
