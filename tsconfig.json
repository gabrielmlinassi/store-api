{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "rootDir": "./",
    "baseUrl": ".",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./typings"],
    "paths": {
      "@/middleware/*": ["middleware/*"],
      "@/schema": ["schema/*"],
      "@/controller/*": ["controller/*"],
      "@/utils": ["utils/*"]
    }
  }
}
